   "source": [
    "import socket\n",
    "s=socket.socket()\n",
    "port=9999\n",
    "s.bind(('localhost',port))\n",
    "s.listen()\n",
    "print('listening')\n",
    "while True:\n",
    "    conn,addr=s.accept()\n",
    "    filename=conn.recv(1024).decode()\n",
    "    files=open(filename,'rb')\n",
    "    filedata=files.read(1024)\n",
    "    conn.send(filedata)\n",
    "    conn.close()"
   ]
  }
