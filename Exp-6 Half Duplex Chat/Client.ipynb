    "s=socket.socket()\n",
    "host=5000\n",
    "s.connect(('localhost',5000))\n",
    "while True:\n",
    "  sentence=input(\">>\")\n",
    "  s.send(sentence.encode())\n",
    "  message=s.recv(2048)\n",
    "  print(message.decode())\n",
    "  if(sentence=='q'):\n",
    "    s.close()"
